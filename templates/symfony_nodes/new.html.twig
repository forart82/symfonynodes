{% extends 'base.html.twig' %}

{% block title %}New SymfonyNodes{% endblock %}

{% block body %}
    <h1>Create new SymfonyNodes</h1>

    {{ include('symfony_nodes/_form.html.twig') }}

    <a href="{{ path('symfony_nodes_index') }}">back to list</a>


<form id="option" action="#" method="POST">
    <label for="types">Types?</label>
    <input type="number" name="types">

    <label for="texts">Texts?</label>
    <input type="number" name="texts">

    <input type="hidden" name="hiddeninput" value="100">
    <input type="submit" form="option" value="send">
</form>

{% endblock %}
